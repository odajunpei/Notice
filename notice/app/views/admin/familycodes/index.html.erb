<div class="container-fuluid">
  <div class="row">
    <div class="col-md-10 offset-1">
      <h1>家族コード一覧</h1>
      <% @familycodes.each do |familycode| %>
      <table>
        <tbody>
        <tr>
          <th>
            <%= familycode.id %>
          </th>
          <td>
              <%= familycode.family_code %>
          </td>
          <td>
            <%= familycode.created_at.strftime('%Y/%m/%d %H:%M:%S') %>
          </td>
        </tbody>
      </table>
<% end %>