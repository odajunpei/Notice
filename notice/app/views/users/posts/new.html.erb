<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <h2 class="mt-3 pb-3">投稿、コメント一覧画面</h2>
      <%= form_with model: @post,url:users_posts_path, local:true do |f| %>
      <table class="table table-borderless mt-5 ">
        <tbody>
          <tr>
            <td><%= f.label :投稿 %></td>
            <td><%= f.text_field :post, value:"おはよう" %></td>
          </tr>
          <tr>
            <td></td>
            <td><%= f.submit "投稿", class: "btn btn-primary"  %></td>
          </tr>
      </tbody>
    </table>
    <% end %>
    </div>
  </div>
</div>