<div class="container-fluid">
  <div class="row">
    <div class="col-xs-12 col-sm-10 offset-sm-1 col-md-8 offset-md-2">
      <h1 class="mt-3">質問内容</h1>
    </div>
     <div class= "col-xs-10 col-sm-10 offset-sm-1 col-md-6 offset-md-3">
      <table class="table tablef">
        <thead>
          <th>質問相手</th>
          <th>質問内容</th>
          <th>質問時間</th>
        </thead>
        <% @questions.each do |question| %>
        <tbody>
          <tr>
            <td>
              <%= question.user_id %>
            </td>
          </tr>
          <tr>
            <td>
              <%= question.body %>
            </td>
          </tr>
          <tr>
            <td>
              <%= question.created_at.strftime('%Y/%m/%d %H:%M:%S') %>
            </td>
          </tr>
          <tr>
            <td>
              <%= @question.choices3 %>
            </td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>